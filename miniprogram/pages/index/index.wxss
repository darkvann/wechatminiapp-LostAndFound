page {
  background-color: #fdf5e0;
}
.body {
  width: 100%;
  height: 1300rpx;
  display: flex;
  flex-direction: column;
  align-items: baseline;
}
.willhide {
  width: 100%;
}
.weui-search {
  width: 100%;
  height: 60rpx;
  padding: 10px 10px;
  background-color: #fdf5e0;
  color: #969696;
  display: flex;
  flex-direction: row;
  /* border: solid black 1rpx; */
}
.weui-search-bar {
  width: 40%;
  height: 40rpx;
  margin-top: 25rpx;
  margin-bottom: 10rpx;
  padding: 10rpx 10rpx;
  background-color: #fdf5e0;
  color: #969696;
  display: flex;
  flex-direction: row;
  /* border: solid black 1rpx; */
}
.search_bar_chosed {
  width: 40%;
  height: 40rpx;
  margin-top: 25rpx;
  margin-bottom: 10rpx;
  padding: 10rpx 10rpx;
  background-color: #fdf5e0;
  color: #fadc90;
  display: flex;
  flex-direction: row;
}
.weui-search-bar-title {
  width: 30%;
  height: 40rpx;
  color: #767676;
  display: flex;
  align-items: center;
  justify-content: center;
  /* border: solid black 1rpx; */
}
.weui-icon-search_in-box {
  height: 50rpx;
  width: 10%;
  display: flex;
  align-items: center;
  justify-content: center;
  /* border: solid black 1rpx; */
}

.weui-search-bar__box {
  width: 80%;
  height: 60rpx;
  display: flex;
  flex-direction: row;
  border: solid #fae3a8 2rpx;
  border-radius: 25rpx;

  display: flex;
  align-items: center;
}
.weui-search-bar__input {
  height: 50rpx;
  width: 87%;
  /* border: solid black 1rpx; */
}
.weui-search-bar__cancel-btn {
  display: flex;
  justify-content: center;
  align-items: center;
  height: 60rpx;
  width: 15%;
  /* border: solid black 1rpx; */
}

.container {
  display: flex;
  align-items: center;
  justify-content: center;
  flex-wrap: wrap;
}
